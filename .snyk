# Snyk (https://snyk.io) policy file, patches or ignores known vulnerabilities.
version: v1.7.1
# ignores vulnerabilities until expiry date; change duration by modifying expiry date
ignore:
  'npm:growl:20160721':
    - mocha > growl:
        reason: None given
        expires: '2017-06-12T15:42:14.067Z'
# patches apply the minimum changes required to fix a vulnerability
patch:
  'npm:ms:20170412':
    - mocha > debug > ms:
        patched: '2017-05-27T23:22:04.892Z'
  'npm:debug:20170905':
    - mocha > debug:
        patched: '2017-09-30T23:22:04.483Z'
